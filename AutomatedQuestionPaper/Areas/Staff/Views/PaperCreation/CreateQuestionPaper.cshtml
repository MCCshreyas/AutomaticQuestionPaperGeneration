@section Scripts
{
	@Scripts.Render("~/bundles/jqueryval")

	<script>

		$(function () {

			var levelValue = @TempData["LevelValue"];
			console.log(levelValue);

			$('span.field-validation-valid.text-danger').hide();
			$('span.field-validation-valid.text-warning').hide();

			$("table").css("border-collapse", "separate");
			$("table").css("border-spacing", "0 1em");


			$('#Question1A option')[0].selected = true;
			$('#Question1B option')[1].selected = true;

			$('#Question2A option')[2].selected = true;
			$('#Question2B option')[3].selected = true;

			$('#Question3A option')[0].selected = true;
			$('#Question3B option')[1].selected = true;

			$('#Question4A option')[2].selected = true;
			$('#Question4B option')[3].selected = true;

			$('#Question5A option')[0].selected = true;
			$('#Question5B option')[1].selected = true;

			$('#Question6A option')[2].selected = true;
			$('#Question6B option')[3].selected = true;


			$("#Question1ALevel").prop('disabled', true);
			$("#Question1BLevel").prop('disabled', true);
			$("#Question2ALevel").prop('disabled', true);
			$("#Question2BLevel").prop('disabled', true);
			$("#Question3ALevel").prop('disabled', true);
			$("#Question3BLevel").prop('disabled', true);
			$("#Question4ALevel").prop('disabled', true);
			$("#Question4BLevel").prop('disabled', true); 
			$("#Question5ALevel").prop('disabled', true);
			$("#Question5BLevel").prop('disabled', true);
			$("#Question6ALevel").prop('disabled', true);
			$("#Question6BLevel").prop('disabled', true);


			$('#Question1ALevel option').eq($("#Question1A")[0].selectedIndex).prop('selected', true);
			$('#Question1BLevel option').eq($("#Question1B")[0].selectedIndex).prop('selected', true);
			$('#Question2ALevel option').eq($("#Question2A")[0].selectedIndex).prop('selected', true);
			$('#Question2BLevel option').eq($("#Question2B")[0].selectedIndex).prop('selected', true);
			$('#Question3ALevel option').eq($("#Question3A")[0].selectedIndex).prop('selected', true);
			$('#Question3BLevel option').eq($("#Question3B")[0].selectedIndex).prop('selected', true);
			$('#Question4ALevel option').eq($("#Question4A")[0].selectedIndex).prop('selected', true);
			$('#Question4BLevel option').eq($("#Question4B")[0].selectedIndex).prop('selected', true);
			$('#Question5ALevel option').eq($("#Question5A")[0].selectedIndex).prop('selected', true);
			$('#Question5BLevel option').eq($("#Question5B")[0].selectedIndex).prop('selected', true);
			$('#Question6ALevel option').eq($("#Question6A")[0].selectedIndex).prop('selected', true);
			$('#Question6BLevel option').eq($("#Question6B")[0].selectedIndex).prop('selected', true);

			$("#Question1A").change(function () {

				var index = $("#Question1A")[0].selectedIndex;

				$('#Question1ALevel option').eq(index).prop('selected', true);

				console.log(parseInt($("#Question1ALevel option:selected").text()));
				console.log(parseInt($("#Question1BLevel option:selected").text()));

				console.log((parseInt($("#Question1ALevel option:selected").text()) +
					parseInt($("#Question1BLevel option:selected").text())));

				if ((parseInt($("#Question1ALevel option:selected").text()) +
						parseInt($("#Question1BLevel option:selected").text())) >
					levelValue) {
					$("select#Question1ALevel").css("border-color", "red");
					$("select#Question1BLevel").css("border-color", "red");
					
				} else {
					$("select#Question1ALevel").css("border-color", "");
					$("select#Question1BLevel").css("border-color", "");
					$("select#Question1ALevel").css("border-color", "");
					$("select#Question1BLevel").css("box-shadow", "");
				}


				if ($("#Question1A option:selected").text() === $("#Question1B option:selected").text()) {
					$("span#Question1AError").show();
					$("span#Question1BError").show();
					$("select#Question1A").css("border-color", "red");
					$("select#Question1B").css("border-color", "red");
				} else {
					$("select#Question1A").css("border-color", "");
					$("select#Question1B").css("border-color", "");
					$("span#Question1AError").hide();
					$("span#Question1BError").hide();
				}

				if ($("#Question1A option:selected").text() === $("#Question2A option:selected").text()) {
					$("span#Question1AError").show();
					$("span#Question2AError").show();
					$("select#Question1A").css("border-color", "red");
					$("select#Question2A").css("border-color", "red");
				} else {
					$("select#Question1A").css("border-color", "");
					$("select#Question2A").css("border-color", "");
					$("span#Question1AError").hide();
					$("span#Question2AError").hide();
